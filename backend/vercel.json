{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/$1"
      }
    ],
    "functions": {
      "api/[[...all]]": {
        "handler": "dist/index.js",
        "runtime": "nodejs18.x"
      }
    }
  }